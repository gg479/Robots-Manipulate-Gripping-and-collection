<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="x" default="3"/>
    <arg name="y" default="3"/>
    <arg name="z" default="3"/>
    
    <node name="spawn_1_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find my_simulations)/models/Boxes_blue.urdf -urdf -x $(arg x) -y $(arg y) -z $(arg z)  -model box1" />

    
    <node name="spawn_2_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find my_simulations)/models/Boxes_red.urdf -urdf -x 5 -y 5 -z 5  -model box2" />

</launch>

 
