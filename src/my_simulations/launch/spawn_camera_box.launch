<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="x" default="0"/>
    <arg name="y" default="0"/>
    <arg name="z" default="3"/>
    
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find my_simulations)/models/camera_box.urdf -urdf -x $(arg x) -y $(arg y) -z $(arg z)  -model camera_box" />
</launch>
